{
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "parser": "babel-eslint", 
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "extends": "plugin:react/recommended",
  "rules": {
    "no-console": 0,
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "quotes": ["warning", "double"],
    "semi": ["warning", "always"],
    "quote-props": ["error", "consistent"]
  }
}
