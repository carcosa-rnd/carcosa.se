  },
  "scripts": {
    "prettier": "prettier --write \"./**/*.{js,jsx,json,html,css,mdx,md,json}\"",
    "sync-dep": "yarn outdated || yarn upgrade-interactive --latest && syncpack fix-mismatches && syncpack list-mismatches && syncyarnlock -ks",
    "develop": "gatsby develop",
    "build": "yarn clean && gatsby build",
    "deploy": "yarn clean && gatsby build --prefix-paths && gh-pages -d public",
    "serve": "gatsby serve",
    "clean": "rimraf .cache public",
    "format": "prettier --write '**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "gh-pages": "^3.2.3",
    "prettier": "^2.3.2",
